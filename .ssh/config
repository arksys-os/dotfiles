# Default github account: david7ce
Host github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/david7ce
   #IdentitiesOnly yes

# Second github account: curiosity432
Host github-curiosity432 # remote url must be git@github-curiosity432:curiosity432/Repo.git
   HostName github.com
   User git
   IdentityFile ~/.ssh/curiosity432
   #IdentitiesOnly yes

