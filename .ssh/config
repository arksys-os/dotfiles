# git@host:username/repo.git = git@github.com:u1-gh/testing.git

# GitHub user 1
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/u1-gh
    IdentitiesOnly yes

# GitHub user 2
Host github-user2
    HostName github.com
    User git
    IdentityFile ~/.ssh/u2-gh
    IdentitiesOnly yes

# GitLab user
Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/u1-gl
    IdentitiesOnly yes

# Codeberg user
Host codeberg.org
    HostName codeberg.org
    User git
    IdentityFile ~/.ssh/u1-cb
    IdentitiesOnly yes
